{
	"name": "adonis5-audit",
	"version": "3.1.0",
	"description": "Audit provider for AdonisJS 5",
	"scripts": {
		"mrm": "mrm --preset=@adonisjs/mrm-preset",
		"dev": "tsc --watch",
		"copy:files": "npx mkdirp ./build/templates && cp -r ./templates/* ./build/templates/",
		"prepare": "npm run build",
		"format": "prettier --write .",
		"version": "npm run build",
		"run-prettier": "prettier --write .",
		"lint": "eslint . --ext=.ts",
		"pretest": "env DEBUG=adonis-audit* && npm run lint && tsc --noEmit",
		"test": "node -r @adonisjs/require-ts/build/register ./japaFile.ts",
		"clean": "rm -rf build",
		"compile": "npm run clean && npm run lint && tsc && npm run copy:files",
		"build": "npm run compile",
		"prepublishOnly": "npm run build",
		"commit": "npm run cm",
		"cm": "cz",
		"cma": "git add . && cz",
		"rl": "npm i && npm link",
		"ri": "npm run release",
		"rici": "release-it --ci",
		"changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0",
		"r": "release-it",
		"r:p": "release-it patch",
		"r:mi": "release-it minor",
		"r:ma": "release-it major"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ks-labs/adonis5-audit.git"
	},
	"keywords": [
		"Adonis",
		"5",
		"adonis-audit"
	],
	"author": "KSLabs",
	"contributors": [
		"KSLabs <desenvolvimento@ksgestao.com.br>",
		"vinicioslc <vinicioslc@outlook.com>"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ks-labs/adonis5-audit/issues"
	},
	"homepage": "https://github.com/ks-labs/adonis5-audit#readme",
	"files": [
		"build/adonis-typings",
		"build/providers",
		"build/src",
		"build/index.d.ts",
		"build/index.js",
		"build/templates"
	],
	"nyc": {
		"exclude": [
			"test"
		],
		"extension": [
			".ts"
		]
	},
	"main": "./build/providers/AdonisAuditProvider.js",
	"types": "./build/adonis-typings/index.d.ts",
	"adonisjs": {
		"types": "adonis5-audit",
		"providers": [
			"adonis5-audit"
		],
		"templates": {
			"basePath": "./build/templates",
			"database": [
				{
					"src": "00000000000_audits.txt",
					"dest": "migrations/00000000000_audits"
				}
			]
		}
	},
	"changelog": {
		"emojis": true,
		"authorName": true,
		"authorEmail": false
	},
	"config": {
		"commitizen": {
			"path": "cz-emoji"
		}
	},
	"np": {
		"contents": ".",
		"anyBranch": false
	},
	"dependencies": {
		"@types/lodash": "^4.14.191",
		"debug": "^4.3.4",
		"lodash": "^4.17.21",
		"luxon": "^3.2.1",
		"node-exceptions": "^4.0.1"
	},
	"peerDependencies": {
		"@adonisjs/events": "^7.1.2"
	},
	"devDependencies": {
		"@adonisjs/auth": "^8.2.3",
		"@adonisjs/core": "^5.8.4",
		"@adonisjs/events": "^7.0.1",
		"@adonisjs/ioc-transformer": "^2.3.2",
		"@adonisjs/lucid": "^18.1.0",
		"@adonisjs/mrm-preset": "^5.0.3",
		"@adonisjs/require-ts": "^2.0.13",
		"@commitlint/cli": "^16.3.0",
		"@commitlint/config-conventional": "^13.1.0",
		"@favoloso/conventional-changelog-emoji": "^0.10.0",
		"@japa/assert": "^1.3.6",
		"@japa/expect": "^2.0.1",
		"@japa/preset-adonis": "^1.2.0",
		"@japa/runner": "^2.2.2",
		"@poppinss/dev-utils": "^2.0.3",
		"@release-it/bumper": "^4.0.0",
		"@types/debug": "^4.1.7",
		"@types/node": "^14.18.36",
		"@types/ramda": "^0.27.62",
		"adonis-provider-tester": "^1.2.0",
		"chai": "^4.3.4",
		"commitizen": "^4.3.0",
		"commitlint-config-gitmoji": "^2.2.5",
		"conventional-changelog-cli": "^2.2.2",
		"conventional-changelog-cz-emoji-config": "^1.0.1",
		"cz-emoji": "^1.3.1",
		"doctoc": "^2.1.0",
		"dotenv": "^10.0.0",
		"eslint": "^8.6.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-adonis": "^2.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"japa": "^4.0.0",
		"mkdirp": "^3.0.1",
		"mrm": "^3.0.10",
		"np": "^7.6.0",
		"npm-audit-html": "^1.5.0",
		"prettier": "^2.5.1",
		"reflect-metadata": "^0.1.13",
		"release-it": "15.3.0",
		"source-map-support": "^0.5.21",
		"sqlite3": "^5.1.4",
		"supertest": "^6.1.6",
		"ts-mockito": "^2.6.1",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.4",
		"yargs": "^15.4.1"
	},
	"directories": {
		"test": "test"
	}
}
